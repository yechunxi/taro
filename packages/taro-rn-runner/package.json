{
  "name": "@tarojs/rn-runner",
  "version": "3.0.8",
  "description": "ReactNative build tool for taro",
  "main": "index.js",
  "scripts": {
    "build": "run-s clean prod copy",
    "dev": "tsc -w",
    "prod": "tsc",
    "copy": "copyfiles -f src/config/*.json dist/config/",
    "clean": "rimraf dist",
    "lint": "tslint src/**/*.ts --fix",
    "lint:typecheck": "tslint -p tsconfig.json src/**/*.ts --fix",
    "prepack": "npm run build",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "keywords": [
    "taro"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "author": "yechunxi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "homepage": "https://github.com/NervJS/taro#readme",
  "devDependencies": {
    "@tarojs/taro": "^3.0.8",
    "jest": "^26.4.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@tarojs/helper": "^3.0.8"
  }
}
